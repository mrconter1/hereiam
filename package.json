{
  "name": "hereiam",
  "version": "1.0.0",
  "description": "A local document search application with natural language queries",
  "main": "hereiam-main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"vite\" \"node_modules\\.bin\\electron.cmd .\"",
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"node_modules\\.bin\\electron.cmd .\"",
    "dev:browser": "vite --host",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "electron-builder",
    "generate-icons": "electron-icon-builder --input=./assets/icon.svg --output=./assets --flatten",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.hereiam.app",
    "productName": "HereIAm",
    "files": [
      "dist/**/*",
      "hereiam-main.js",
      "src/main/**/*",
      "assets/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
  "keywords": [
    "search",
    "document",
    "vector",
    "embedding",
    "electron",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "canvas": "^3.1.0",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron-icon-builder": "^2.0.1",
    "electron-is-dev": "^3.0.1",
    "vite": "^6.2.2",
    "wait-on": "^8.0.3"
  }
}
